{
  "name": "hack-the-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "firebase": "^5.8.2",
    "highlightjs": "^9.12.0",
    "normalize.css": "^8.0.1",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^1.0.0",
    "primereact": "^3.0.0",
    "quill": "^1.3.6",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "react-transition-group": "^2.5.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/components --include-path ./node_modules src/components/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/components --include-path ./node_modules src/components/ -o src/styles --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5"
  }
}
